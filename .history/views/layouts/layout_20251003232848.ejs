<!DOCTYPE html>
<html lang="en">
<%- include('../partials/head') %>
<body>
    <div id="wrapper">
        <%- include('../partials/header') %>
        <%- include('../partials/navigation') %>

        <main>
            <% if (messages && messages.length > 0) { %>
            <ul>
                <% messages.forEach(function(message) { %>
                <li><%= message %></li>
                <% }) %>
            </ul>
            <% } %>
            <%- body %>
        </main>

        <%- include('../partials/footer') %>
    </div>
</body>
</html>